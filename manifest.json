{
  "manifest_version": 3,
  "name": "Ghi chú thông minh",
  "version": "1.0",
  "description": "Ghi chú với thông báo tự động và đồng bộ Google",
  "permissions": ["storage", "notifications", "alarms"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
